pod 'FacebookLogin'
pod 'FacebookCore'
pod 'SwiftyXMLParser'
pod 'Cosmos'
pod 'Kanna'

  target 'ShowcaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ShowcaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
